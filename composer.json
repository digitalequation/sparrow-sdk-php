{
    "name"       : "digitalequation/sparrow-sdk",
    "description": "Sparrow PHP SDK",
    "keywords"   : [
        "sparrow",
        "api"
    ],
    "homepage": "https://sparrowone.com/",
    "license" : "Proprietary",
    "authors" : [
        {
            "name" : "Marcel Baker",
            "email": "marcel.baker@digitalequation.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "SparrowSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SparrowSDK\\Tests\\": "tests"
        }
    },
    "suggest": {
        "laravel/framework": "Allows the use of Laravel specific Facades"
    }
}
